
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path 'platform/qca955x_wmac'
	option htmode 'HT20'
	option txpower '24'
	option country 'US'
	option channel 'auto'
	option disabled '0'

config wifi-iface
	option device 'radio0'
	option mode 'ap'
	option encryption 'none'
	option network 'wifi'
	option ssid 'HotspotSystem.com'

